div.nav {
    width: 100%;
    float: left;
    margin 0 0 lem 0;
    padding: 0;
    background-color: #7A991A;
    border-bottom: 1px solid #ccc;
}
div.nav ul {
    list-style: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
}
div.nav li {
    display: inline;
    float: left;     
}
div.nav a:link, div.nav a:visited {
    display: block;
    width: 100px;
    font-weight: bold;
    padding: 4px;
    text-decoration: none;
    text-align: center;
    color: #00FFA6;
    background-color: #2600FF;
}
div.nav a:hover, div.nav a:active {
    background-color: #7A991A;
}
